// RUN: %parse-tree --mode tree %s | FileCheck %s

package main

struct A {}
// CHECK: struct_decl struct A

public struct B {}
// CHECK: struct_decl
// CHECK-NEXT: public
// CHECK-SAME: struct B

struct C {
    field1: i32
}

// CHECK: struct C
// CHECK-NEXT: struct_field field1
// CHECK-NEXT: type i32

struct D {
    x: f64
    y: f64
}

// CHECK: struct D
// CHECK-NEXT: struct_field x
// CHECK-NEXT: type f64
// CHECK-NEXT: struct_field y
// CHECK-NEXT: type f64

struct E {
    val: i32 = 10
    other = 0.1
}

// CHECK: struct E
// CHECK-NEXT: struct_field val
// CHECK-NEXT: type i32
// CHECK-NEXT: initializer = 
// CHECK: num_literal 10
// CHECK-NEXT: struct_field other
// CHECK-NEXT: initializer = 
// CHECK: num_literal 0.1


